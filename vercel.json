{
    "version": 2,
    "buildCommand": "npm install --prefix prooflearn/frontend && npm run build --prefix prooflearn/frontend",
    "outputDirectory": "prooflearn/frontend/dist",
    "rewrites": [
        {
            "source": "/api/ai",
            "destination": "https://prooflearn-backend2.onrender.com/api/ai"
        },
        {
            "source": "/api/(.*)",
            "destination": "https://prooflearn-backend2.onrender.com/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}