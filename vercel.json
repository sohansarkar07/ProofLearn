{
    "version": 2,
    "outputDirectory": "prooflearn/frontend/dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/prooflearn/frontend/dist/index.html"
        }
    ],
    "functions": {
        "api/index.js": {
            "includeFiles": "prooflearn/backend/**"
        }
    }
}